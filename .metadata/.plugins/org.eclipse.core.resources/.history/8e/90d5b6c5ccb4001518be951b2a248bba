package Chapter3;

import DataStructures.*;

public class Question3<T>{

	private LinkedList<T> stack;

	public Question3(){
		this.stack = new LinkedList<T>();
	}
	
	public boolean empty(){
		return this.stack.size() == 0;
	}
	
	public T peek(){
		if(this.stack.getTail() == null){
			return null;
		}
		return this.stack.getTail().getData();
	}
	
	public T pop(){
		if(this.empty()){
			return null;
		}
		LinkedListNode<T> val = this.stack.removeTail();
		return val.getData();
	}
	
	public T push(T item){
		if(item == null){
			return null;
		}
		
		this.stack.appendToTail(item);
		
		return item;
	}
	
	public int size(){
		return this.stack.size();
	}
}
