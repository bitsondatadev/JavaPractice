package DataStructures;


public class BinaryTreeNode<T extends Object> extends TreeNode<T>{

	private List<BinaryTreeNode<T>> children = null;
	private T data;
	protected final int m;
	

	public BinaryTreeNode(T data){
		this.children = new ArrayList<BinaryTreeNode<T>>();
		this.m = 2;
		this.setData(data);	
	}


	@Override
	public boolean appendChild(BinaryTreeNode<T> child) {
		if(this.children.size() == this.m ){
			return false;
		}
		
		return this.children.add(child);
	}
	
	public boolean removeChild(BinaryTreeNode<T> child) {
		if(this.children.size() >= this.m ){
			return false;
		}
		
		return this.children.add(child);
	}

}