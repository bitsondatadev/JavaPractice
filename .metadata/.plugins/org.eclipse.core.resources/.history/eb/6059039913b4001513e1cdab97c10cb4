package DataStructures;

public class Stack<T>{

	private LinkedList<T> linkedList;

	public Stack(){
		this.linkedList = new LinkedList<T>();	
	}
	
	public boolean empty(){
		return this.linkedList.size() == 0;
	}
	
	public T peek(){
		return (T) this.linkedList.getTail();
	}
	
	public T pop(){
		if(this.empty()){
			return null;
		}
	}

}
