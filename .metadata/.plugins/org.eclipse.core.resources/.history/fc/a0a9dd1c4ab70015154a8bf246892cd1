package Chapter8;

public class Question3{
	public Question3(){
	}
	
	public int findMagicIndex(int[] A){
		if(A==null){ return -1;}
		int low = 0;
		int high = A.length;
		int mid;
		while(high > low){
			mid = (low + high)/2;
			if(A[mid] < mid){
				low = mid + 1;
			}else if(A[mid] > mid){
				high = mid - 1;
			}else{
				return mid;
			}
		}
		
		return -1;
	}
}


