package Chapter8;



public class Question3{
	private boolean[][] memo;
	public Question3(){
	}
	

	
	public int findMagicIndex(int[] A){
		int low = 0;
		int high = A.length;
		int mid = (low + high)/2;
		while(high > low){
			
			if(A[mid] < mid){
				low = mid + 1;
			}else if(A[mid] > mid){
				high = mid - 1;
			}else{
				return mid;
			}
		}
		
		return -1;
	}
	


}


