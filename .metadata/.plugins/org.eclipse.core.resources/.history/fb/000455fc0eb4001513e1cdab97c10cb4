package DataStructures;

public class Stack<T>{

	private Stack<T> next = null;
	private Stack<T> prev = null;
	private T data;

	public Stack(T data){
		this.data = data;	
	}

	public Stack<T> getNext(){
		return this.next;
	}

	public void setNext(Stack<T> newNext){
		this.next = newNext;
	}

	public Stack<T> getPrev(){
		return this.prev;
	}

	public void setPrev(Stack<T> newPrev){
		this.prev = newPrev;
	}

	public T getData(){
		return this.data;
	}

	public void setData(T newData){
		this.data = newData;
	}
	
	public String toString(){
		return this.data.toString();
	}
}
