package Chapter8;

import java.util.List;
import java.util.ArrayList;

public class Question2{
	
	public Question2(){
	}
	

	public List<Point> getPath(boolean[][] grid){
		return computeCount(0);
	}
	
	private int getPath(int sum){
		if(this.memo[sum] != -1){
			return this.memo[sum];
		}
		
		for(int i=1; i <= Question2.MAX_NUMBER_HOPS; i++){
			if(sum + i <= this.n){
				if(this.memo[sum] == -1){
					this.memo[sum] = computeCount(sum + i);
				}else{
					this.memo[sum] += computeCount(sum + i);
				}
			}
		}
		
		return this.memo[sum];
	}
	

	
}

class Point{
	
	private int x;
	private int y;
	
	Point(int x, int  y){
		this.x = x;
		this.y = y;
	}
	
	public int getX() {
		return x;
	}
	
	public int getY() {
		return y;
	}
}
